<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Project1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Project1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Project1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Project1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="corners_img_profil" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>
    <style name="button_plat_design" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerRadius">22dp</item>
        <item name="cornerSize">22dp</item>
    </style>
    <style name="CustomTheme" parent="">
        <item name="android:colorPrimary">@color/recycle_color</item>
    </style>
    <style name="img_account" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">44dp</item>
    </style>
    <style name="CircularDeterminateProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/background_item_centre</item>
    </style>
    <style name="progressbar_">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/rounded_img_account</item>

    </style>
    <style name="account_scroll_color" parent="">

        <item name="colorPrimary">#008DFD</item>
    </style>


</resources>